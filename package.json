{
  "name": "apiifood",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "start": "tsc && node dist/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:undo": "sequelize-cli db:migrate:undo",
    "db:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:status": "sequelize-cli db:show",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "db:recreate": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "db:new": "sequelize-cli migration:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.11.0",
    "bullmq": "^5.56.9",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "mysql2": "^3.14.3",
    "node-cron": "^4.2.1",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "@types/swagger-ui-express": "^4.1.8",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
